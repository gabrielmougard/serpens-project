    <joint name="fixed_rotation" type="fixed">
        <parent link="world"/>
        <child link="base_link_rotation"/>
    </joint>

    <joint name="link_1_fixed" type="fixed">
        <parent link="base_link_rotation"/>
        <child link="link_1_rot_out"/>
    </joint>
<!--
    <joint name="link_1_motor" type="revolute">
        <parent link="base_link_rotation" />
        <child link="link_1_rot_in" />
        <axis xyz="0 0 1" />
        <origin xyz="${BASE_ORIGIN_X} ${BASE_ORIGIN_Y} ${HEIGHT_1 * 2}" rpy="0 0 ${M_PI / 2}"/>
        <limit effort="1000" lower="${-M_PI / 2}" upper="${M_PI / 2}" velocity="1" />
    </joint>

    <joint name="link_1_spring" type="revolute">
        <parent link="link_1_rot_in" />
        <child link="link_1_rot_out" />
        <axis xyz="0 0 1" />
        <origin xyz="0 0 0" />
        <dynamics damping="0.2"/>
        <limit effort="10" lower="-0.4" upper="0.4" velocity="1" />
    </joint>
-->
    <joint name="link_2_motor" type="revolute">
        <parent link="link_1_rot_out" />
        <child link="link_2_rot_in" />
        <axis xyz="0 0 1" />
        <origin xyz="0 0 ${LENGTH_4}" rpy="0 -${M_PI / 2} 0"/>
        <limit effort="1000.0" lower="${-M_PI / 2}" upper="${M_PI / 2}" velocity="1.0" />
    </joint>

    <joint name="link_2_spring" type="revolute">
        <parent link="link_2_rot_in" />
        <child link="link_2_rot_out" />
        <axis xyz="0 0 1" />
        <origin xyz="0 0 0" />
        <dynamics damping="1"/>
        <limit effort="10" lower="-0.4" upper="0.4" velocity="1" />
    </joint>
